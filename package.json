{
  "name": "tvicl-real-estate-backend",
  "version": "1.0.0",
  "description": "Backend for TVICL Real Estate App â€” Express + MongoDB (Mongoose), JWT auth, file uploads (Cloudinary), Swagger, Zod validation, production-ready utilities",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "imports": {
    "#controllers/*": "./src/controllers/*.js",
    "#models/*": "./src/models/*.js",
    "#routes/*": "./src/routes/*.js",
    "#middleware/*": "./src/middleware/*.js",
    "#utils/*": "./src/utils/*.js",
    "#config/*": "./src/config/*.js",
    "#services/*": "./src/services/*.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --watch src --exec \"node src/index.js\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "check:node": "node -v"
  },
  "keywords": [
    "express",
    "mongoose",
    "real-estate",
    "rest",
    "tvicl"
  ],
  "author": "Felix Vincent",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.35.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.8.0",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "nodemon": "^3.0.1"
  }
}
